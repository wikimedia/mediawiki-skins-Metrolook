[project]
	description = MediaWiki skin Metrolook
[access]
	inheritFrom = mediawiki/skins
[receive]
	requireChangeId = true
[submit]
	mergeContent = true
[access "refs/*"]
	owner = group Administrators
	owner = group mediawiki
	owner = group mediawiki-skins-Metrolook
	push = group mediawiki-skins-Metrolook
[allowSuperproject "mediawiki/skins"]
	matching = refs/heads/master:refs/heads/master
